<div class='feed-container'>
	<h1>Welcome <%= @user.full_name %></h1>
	<%= render template: 'posts/new' %>
	<% unless @posts.empty? %>
		<%= render template: 'posts/index' %>
	<% end %>
	<%= @pending_requests.empty? %>
	<% @pending_requests.each do |request| %>
		<p><%=  request %></p>
	<% end %>
</div>
