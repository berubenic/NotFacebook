<% unless user.profile_image.attached? %>
    <%= image_tag 'default_profile.svg', width: '64px', height: '64px', class: 'nav-profile-image'%>
  <% else %>
    <%= image_tag user.profile_image.variant(resize_to_limit: [64, nil]), width: 'auto', height: '64px', class: 'nav-profile-image'%>
  <% end %>